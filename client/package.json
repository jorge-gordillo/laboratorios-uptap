{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 80",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 80",
    "electron": "concurrently \"set ELECTRON_IS_DEV=0 && npm run preview\" \"wait-on http://localhost:80 && electron .\"",
    "pack": "vite build && set ELECTRON_IS_DEV=0 && electron-builder --dir",
    "dist": "vite build && set ELECTRON_IS_DEV=0 && electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.0",
    "@mui/x-data-grid": "^5.11.1",
    "axios": "^0.27.2",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.2",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "electron-is-dev": "^2.0.0",
    "vite": "^2.9.9",
    "wait-on": "^6.0.1"
  }
}
